{
    "name": "plotfast",
    "version": "0.0.0",
    "description": "High performance data plotting with Canvas",
    "author": "Greg Brisebois",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/gregoryjjb/plotfast.git"
    },
    "private": false,
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "start": "webpack-dev-server --inline --hot --mode development",
        "clean": "shx rm -rf dist && shx rm -rf min",
        "transpile": "tsc",
        "pack": "webpack --mode production",
        "build": "npm run clean && npm run transpile && npm run pack",
        "pretty": "prettier --write \"**/*.{ts,tsx,js,jsx,json}\"",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@babel/core": "^7.0.0",
        "@babel/preset-env": "^7.0.0",
        "@types/node": "^10.12.9",
        "@types/react": "^16.8.1",
        "babel-loader": "^8.0.0",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "file-loader": "^2.0.0",
        "html-webpack-plugin": "^3.2.0",
        "prettier": "^1.15.2",
        "react": "^16.7.0",
        "react-dom": "^16.7.0",
        "shx": "^0.3.2",
        "ts-loader": "^5.3.0",
        "typescript": "^3.1.6",
        "webpack": "^4.17.1",
        "webpack-cli": "^3.1.0",
        "webpack-dev-server": "^3.1.8"
    },
    "dependencies": {
        "downsample-lttb": "^0.0.1"
    },
    "peerDependencies": {
        "react": "^16.7.0",
        "react-dom": "^16.7.0"
    }
}
